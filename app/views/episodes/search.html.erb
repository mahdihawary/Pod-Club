<%= form_tag(episodes_search_path, method: :get) do %>
    <%= label_tag(:q, "Search for:") %>
    <%= text_field_tag(:q) %>
    <%= submit_tag("Search") %>
  
<% end %>

<% if @results.present? %>
  <h1>Songs Found</h1>
  <ul>
    <% @results.each do |song| %>
      <li><%= song["name"] %></li>
    <% end %>
  </ul>
<% end %>



